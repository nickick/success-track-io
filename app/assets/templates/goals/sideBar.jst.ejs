<% var height = ($(window).height() / totalGoals - 5); %>
<% if (goals.length > 3) {%>
  <% goals.each(function(goal) { %>
    <a href="#goal-<%= goal.escape('id') %>">
      <div style= 'height: 40px' class='goal-node-container'>
        <li id='<%= goal.escape('id') %>' class="goal-node" title="<%= goal.escape('name')%>">
          <div><%= goal.escape('name') %></div>
        </li>
      </div>
    </a>
  <% }); %>
<% }; %>
